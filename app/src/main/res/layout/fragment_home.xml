<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".buttonNavigationActivites.HomeFragment">
<androidx.constraintlayout.widget.ConstraintLayout

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".buttonNavigationActivites.HomeFragment"
    android:background="#d2fcfa">

    <ImageView
        android:id="@+id/maountains"
        android:layout_width="300dp"
        android:layout_height="220dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/maountains"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/mind_icon"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:background="@drawable/mind_icon"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="271dp" />

    <ImageView
        android:id="@+id/self_awarness_icon"
        android:layout_width="170dp"
        android:layout_height="120dp"
        android:background="@drawable/self_awarness_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/clouds"
        android:layout_width="170dp"
        android:layout_height="120dp"
        android:background="@drawable/clouds"
        app:layout_constraintEnd_toStartOf="@+id/self_awarness_icon"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="465dp" />

    <Button
        android:id="@+id/personal_BrandingBtn"
        android:layout_width="178dp"
        android:layout_height="54dp"
        android:background="@drawable/rounded_corners"
        android:text="Personal-Branding"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.819"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.32" />

    <Button
        android:id="@+id/mindMappingBtn"
        android:layout_width="169dp"
        android:layout_height="58dp"
        android:background="@drawable/rounded_corners"
        android:text="Mind Mapping"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.594" />

    <Button
        android:id="@+id/selfAwarenessBtn"
        android:layout_width="151dp"
        android:layout_height="47dp"
        android:background="@drawable/rounded_corners"
        android:text="Self awarness"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.838"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.879" />

</androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>