<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".buttonNavigationActivites.HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#d2fcfa"
        tools:context=".buttonNavigationActivites.HomeFragment">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="338dp"
            android:layout_height="51dp"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/rounded_corners"
            app:layout_constraintBottom_toTopOf="@+id/maountains"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="22dp"
            android:layout_height="19dp"
            android:layout_marginEnd="184dp"
            android:layout_marginBottom="2dp"
            android:background="@drawable/icon"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            app:layout_constraintEnd_toEndOf="@+id/imageView"
            app:layout_constraintLeft_toRightOf="@id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintVertical_bias="0.6" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="22dp"
            android:layout_height="19dp"
            android:background="@drawable/icon1"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            app:layout_constraintEnd_toStartOf="@+id/imageView2"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintVertical_bias="1.0" />

        <ImageView
            android:background="@drawable/icon2"
            android:layout_width="22dp"
            android:layout_height="19dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView2"
            app:layout_constraintEnd_toStartOf="@+id/imageView3"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Yourself Journey 30%"
            android:textColor="#1f314a"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView"
            app:layout_constraintHorizontal_bias="0.267"
            app:layout_constraintStart_toEndOf="@+id/profile_image"
            app:layout_constraintTop_toTopOf="@+id/imageView" />

        <TextView
            android:layout_width="58dp"
            android:layout_height="21dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="1dp"
            android:text="Rank"
            android:textColor="#1f314a"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            app:layout_constraintEnd_toEndOf="@+id/imageView"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="@+id/imageView" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profile_image"
            android:layout_width="41dp"
            android:layout_height="45dp"
            android:layout_marginEnd="284dp"
            android:src="@drawable/profile1"
            app:civ_border_color="#FF000000"
            app:civ_border_width="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            app:layout_constraintEnd_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="@+id/imageView"
            app:layout_constraintVertical_bias="0.666" />

        <ImageView
            android:id="@+id/maountains"
            android:layout_width="301dp"
            android:layout_height="222dp"
            android:layout_marginTop="94dp"
            android:background="@drawable/maountains"
            app:layout_constraintBottom_toTopOf="@+id/mind_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/lines"
            android:layout_width="35dp"
            android:layout_height="15dp"
            android:layout_marginTop="340dp"
            android:background="@drawable/lines"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.904"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/lines3"
            android:layout_width="220dp"
            android:layout_height="90dp"
            android:background="@drawable/line3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.018"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/self_awarness_icon" />

        <ImageView
            android:id="@+id/tree"
            android:layout_width="30dp"
            android:layout_height="100dp"
            android:background="@drawable/tree"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.942"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.711" />

        <ImageView
            android:id="@+id/mind_icon"
            android:layout_width="270dp"
            android:layout_height="130dp"
            android:layout_marginBottom="72dp"
            android:background="@drawable/mind_icon"
            android:layout_marginTop="30dp"
            app:layout_constraintBottom_toTopOf="@+id/self_awarness_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.418"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/maountains" />

        <ImageView
            android:id="@+id/self_awarness_icon"
            android:layout_width="160dp"
            android:layout_height="100dp"
            android:layout_marginBottom="120dp"
            android:background="@drawable/self_awarness_icon"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.589"
            app:layout_constraintTop_toBottomOf="@+id/mind_icon"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/line2"
            android:layout_width="50dp"
            android:layout_height="150dp"
            android:layout_marginBottom="324dp"
            android:background="@drawable/line2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.044"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/line4"
            android:layout_width="50dp"
            android:layout_height="80dp"
            android:layout_marginBottom="488dp"
            android:background="@drawable/line4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.044"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/line8"
            android:layout_width="50dp"
            android:layout_height="80dp"
            android:layout_marginBottom="172dp"
            android:background="@drawable/line4"
            android:rotation="180"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.825"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/line"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginBottom="260dp"
            android:background="@drawable/line4"
            android:rotation="90"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.825"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/clouds"
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:background="@drawable/clouds"
            app:layout_constraintBottom_toBottomOf="@+id/self_awarness_icon"
            app:layout_constraintEnd_toStartOf="@+id/self_awarness_icon"
            app:layout_constraintHorizontal_bias="0.625"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/self_awarness_icon" />

        <Button
            android:id="@+id/personal_BrandingBtn"
            android:layout_width="178dp"
            android:layout_height="54dp"
            android:background="@drawable/rounded_corner_locked"
            android:text="Personal-Branding"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.819"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.345"
            android:elevation="10dp"/>

        <Button
            android:id="@+id/mindMappingBtn"
            android:layout_width="169dp"
            android:layout_height="58dp"
            android:background="@drawable/rounded_corner_locked"
            android:text="Mind Mapping"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.533"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.594" />

        <Button
            android:id="@+id/selfAwarenessBtn"
            android:layout_width="151dp"
            android:layout_height="47dp"
            android:background="@drawable/rounded_corners"
            android:text="Self awarness"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.838"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.865" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>